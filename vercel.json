{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "Frontend/dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/Backend/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "functions": {
      "Backend/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "env": {
      "NODE_ENV": "production"
    }
  }